{
	"trailingComma": "none",
	"tabWidth": 2,
	"useTabs": false,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"arrowParens": "avoid",
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"/pages/",
		"/components/",
		"/services/",
		"/hooks/",
		"/store/",
		"/types/",
		"/*.module.scss",
		"/*.scss",
		"^../(.*)",
		"^./(.*)"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
